{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "no-shadow": "off",
    "space-before-function-paren": "off",
    "import/no-dynamic-require": "off",
    "global-require": "off",
    "comma-dangle": "off"
  },
  "globals": {
    "document": true
  },
  "env": {
    "jquery": true
    }
}
